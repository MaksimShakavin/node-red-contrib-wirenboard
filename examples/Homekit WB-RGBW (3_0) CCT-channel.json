    {
        "id": "7da813aaba2060cd",
        "type": "function",
        "z": "d60a5ec574524af9",
        "g": "d76d38a267cfa5c2",
        "name": "to homekit",
        "func": "var isOn = parseInt(msg.payload[\"/devices/wb-mrgbw-d3-02_27/controls/CCT enable\"]);\nvar bri = parseInt(msg.payload[\"/devices/wb-mrgbw-d3-02_27/controls/CCT brightness\"]);\nvar ct = parseInt(msg.payload[\"/devices/wb-mrgbw-d3-02_27/controls/CCT temperature\"]);\nlet ctHomekit = convertRange(ct, [0, 100], [140, 500]);\nctHomekit = 500 - ctHomekit + 140;\n\nvar newMsg = {};\nnewMsg.payload_in = msg.payload;\nnewMsg.payload = {\n    \"Brightness\": bri,\n    \"On\":isOn,\n    \"ColorTemperature\": ctHomekit\n};\n\nreturn newMsg;\n\n\nfunction convertRange(value, r1, r2) {\n    var val = Math.ceil((value - r1[0]) * (r2[1] - r2[0]) / (r1[1] - r1[0]) + r2[0]);\n    if (val < r2[0]) val = r2[0];\n    if (val > r2[1]) val = r2[1];\n    return val;\n}",
